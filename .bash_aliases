# general aliases
alias ..='cd ..'
alias ls='ls --color=auto'
alias ll='ls -a'
alias l='ls -CF'

#Raspberry Pi real vnc headless server startup
alias realvnc='vncserver -randr=1920x1080'

#c9 specific aliases
